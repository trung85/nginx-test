
{
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "app", 
            "image": "$AWS_ACCOUNT.dkr.ecr.$AWS_REGION.amazonaws.com/cuongtv:%IMAGE_TAG%", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256", 
    "memory": "512"
}
